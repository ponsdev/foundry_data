{"_id":"ra8gI7BD0j49hDN6","name":"Prismatic Spray","permission":{"default":0,"devnIbfBHb74U9Zv":3},"type":"spell","data":{"description":{"value":"","chat":"","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":""},"target":{"value":60,"units":"ft","type":"cone"},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"dex","dc":null,"scaling":"spell"},"level":7,"school":"abj","components":{"value":"","vocal":false,"somatic":false,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"prepared","prepared":true},"scaling":{"mode":"level","formula":"1d8"},"attributes":{"spelldc":10}},"flags":{"dynamiceffects":{"effects":[{"modSpecKey":"macro.itemMacro","value":"@target @spellLevel @item","mode":"+","targetSpecific":false,"id":1,"itemId":"8tAFQog6gg0Tkvzy","active":true,"_targets":[],"label":"Macro Item Macro"}],"equipActive":false,"alwaysActive":false,"activeEquipped":false},"itemacro":{"macro":{"data":{"name":"Primatic Spray","type":"script","scope":"global","command":"console.warn(\"Here I am\")\r\nlet doDamage = (damageType, color, target) => {\r\n  let damageRoll = new Roll(\"10d6\").roll()\r\n  const flavor = `Prismatic Spray ${color} - ${target.name} (${CONFIG.DND5E.damageTypes[damageType]}) Damage`\r\n  new MidiQOL.DamageOnlyWorkflow(actor, token, damageRoll.total, damageType, [target], damageRoll, {flavor})\r\n}\r\n\r\nif (args[0] === \"on\") {\r\n  let target = canvas.tokens.get(args[1])\r\n  let effects = [new Roll(\"1d8\").roll().total];\r\nconsole.warn(\"Effects are \", effects)\r\n  if (effects[0] === 8) {\r\n    effects = [new Roll(\"1d7\").roll().total, new Roll(\"1d7\").roll().total];\r\nconsole.warn(\"effects are \", effects);\r\n  }\r\n  effects.forEach(efnum => {\r\n    switch(efnum) {\r\n    case 1:\r\n      return doDamage(\"fire\", \"Red\", target);\r\n    case 2:\r\n      return doDamage(\"acid\", \"Orange\", target)\r\n    case 3:\r\n      return doDamage(\"lightning\", \"Yellow\", target);\r\n    case 4:\r\n      return doDamage(\"poison\", \"Green\", target);\r\n    case 5:\r\n      return doDamage(\"cold\", \"Blue\", target);\r\n    case 6:\r\n      return ChatMessage.create({flavor: `Prismatic Spray - Indigo`, content: `${target.name} is restrained. make a Constitution saving throw at the end of each of ${target.name}'s turn`})\r\n    case 7:\r\n      return ChatMessage.create({flavor: `Prismatic Spray - Violet`, content: `${target.name} is blinded. Make a Wisdom saving throw at the start of ${target.name}'s next turn`})\r\n    }\r\n  })\r\n}\r\n","author":"devnIbfBHb74U9Zv"},"options":{},"apps":{},"compendium":null}},"favtab":{"isFavorite":true},"exportSource":{"world":"testWorld","system":"dnd5e","coreVersion":"0.6.6","systemVersion":0.96},"dae":{"activeEquipped":false,"alwaysActive":false},"core":{"sourceId":"Item.Cwed6QSIGGtisw7A"}},"img":"icons/megaWowPack/Spell_Frostfire%20Orb.png","effects":[{"_id":"gOnQpt6PVE9KUDZl","flags":{},"changes":[{"key":"macro.itemMacro","value":"@target @spellLevel","mode":0,"priority":0}],"duration":{},"icon":"icons/megaWowPack/Spell_Frostfire%20Orb.png","label":"Prismatic Spray Active Effects","transfer":false}]}
