":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":10,"areaType":"burst"},"time":{"value":"2"},"duration":{"value":"5 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"UB8aoEcw8sIPGBag","heightenedLevel":2},"hasCounteractCheck":{"value":false},"areasize":{"value":"10-foot burst"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"vkXZ1CDW49ufIquY":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.HRb2doyaLtaoCfi3"}}},{"_id":"am6n9d2igkXrJDIj","name":"Lightning Rod","type":"action","img":"systems/pf2e/icons/actions/Reaction.webp","data":{"description":{"value":"<p><strong>Trigger</strong> An ability, thunderstorm, or spell the knefen does not control would deal electricity damage within 100 feet of the knefen.</p>\n<p><strong>Effect</strong> The electricity arcs to the knefen instead, dealing its damage in a <span data-pf2-effect-area=\"burst\" data-pf2-distance=\"5\">5-foot burst</span> centered on the knefen. Creatures in the area must attempt a <span data-pf2-check=\"reflex\" data-pf2-traits=\"damaging-effect,electricity\" data-pf2-label=\"\" data-pf2-dc=\"17\" data-pf2-show-dc=\"gm\">basic Reflex</span> save.</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-07T19:31:19.309-04:00","version":{"schema":0.692,"foundry":"9.269","system":"3.13.5.16"}}},"actionType":{"value":"reaction"},"actionCategory":{"value":"offensive"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"vkXZ1CDW49ufIquY":3},"flags":{}}],"effects":[],"folder":null,"sort":0,