{"_id":"qPhrMRuJ3ekGqbKV","name":"Officer's Regalia","type":"armor","img":"systems/pf2e/icons/equipment/adventuring-gear/clothes-explorers.webp","data":{"description":{"value":"<p>This fine clothing is adorned with detailed embroidery displaying the symbols of Lastwall and its capital, Vigil. It also carries a +1 armor potency rune. While worn, it grants you a +1 item bonus to Diplomacy, Intimidation, and Society skill checks.</p>"},"source":{"value":""},"traits":{"value":["abjuration","invested","magical","comfort"],"rarity":"uncommon","custom":""},"rules":[{"key":"FlatModifier","selector":"intimidation","type":"item","value":1},{"key":"FlatModifier","selector":"diplomacy","type":"item","value":1},{"key":"FlatModifier","selector":"society","type":"item","value":1}],"slug":"clothing-explorers","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-07T19:31:04.124-04:00","version":{"schema":0.692,"foundry":"9.269","system":"3.13.5.16"}}},"quantity":1,"baseItem":"explorers-clothing","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"1"},"equippedBulk":{"value":"L"},"price":{"value":{"gp":200}},"equipped":{"carryType":"worn","invested":false,"handsHeld":0,"inSlot":false},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Explorer's Clothing","img":"systems/pf2e/icons/unidentified_item_icons/armor.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"wornarmor"},"level":{"value":6},"armor":{"value":0},"category":"unarmored","group":"cloth","strength":{"value":0},"dex":{"value":5},"check":{"value":0},"speed":{"value":0},"potencyRune":{"value":1},"resiliencyRune":{"value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"def