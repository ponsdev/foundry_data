{"_id":"jC8GmH0Un6vDxdMj","data":{"MAP":{"value":""},"ability":{"value":"dex"},"baseItem":"alchemical-bomb","bonus":{"value":2},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"acid","dice":3,"die":"d6","value":""},"description":{"chat":"","unidentified":"","value":"<p><strong>Activate</strong> <span class=\"pf2-icon\">A</span> Strike</p>\n<p>This flask filled with corrosive acid deals 1 acid damage, 3d6 persistent acid damage, and 3 acid splash damage. You gain a +2 item bonus to attack rolls.</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"value":"bomb"},"hands":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identification":{"status":"identified"},"identified":{"value":true},"invested":{"value":false},"level":{"value":11},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":0},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"250 gp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","strikeConditionType":"","strikeConditionValue":null,"value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"value":1},"range":{"value":"20"},"reload":{"value":""},"rules":[],"size":{"value":"med"},"source":{"value":""},"splashDamage":{"value":3},"stackGroup":{"value":""},"strikingRune":{"value":""},"traits":{"custom":"","rarity":{"value":"common"},"value":["acid","alchemical","bomb","consumable","splash"]},"usage":{"value":"held-in-one-hand"},"weaponType":{"value":"martial"},"weight":{"value":"L"},"slug":"acid-flask-greater"},"effects":[],"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.jC8GmH0Un6vDxdMj"}},"img":"systems/pf2e/icons/equipment/alchemical-items/alchemical-bombs/a