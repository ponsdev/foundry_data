{"_id":"PnFfW5u24xZV6mOH","data":{"MAP":{"value":""},"ability":{"value":"str"},"baseItem":null,"bonus":{"value":0},"bonusDamage":{"value":0},"brokenThreshold":{"value":0},"bulkCapacity":{"value":""},"collapsed":{"value":false},"containerId":{"value":""},"damage":{"damageType":"bludgeoning","dice":1,"die":"d4","value":""},"description":{"chat":"","unidentified":"","value":"<p>Poi are light weights tethered to ropes or chains. Performers swing the weights, usually one in each hand, in rhythmic patterns.</p>"},"equipped":{"value":false},"equippedBulk":{"value":""},"group":{"value":"flail"},"hands":{"value":""},"hardness":{"value":0},"hp":{"value":0},"identification":{"status":"identified"},"identified":{"value":true},"invested":{"value":false},"level":{"value":"0"},"maxHp":{"value":0},"negateBulk":{"value":"0"},"potencyRune":{"value":0},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":"2 sp"},"property1":{"critDamage":"","critDamageType":"","critDice":null,"critDie":"","criticalConditionType":"","criticalConditionValue":null,"damageType":"","dice":null,"die":"","strikeConditionType":"","strikeConditionValue":null,"value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"quantity":{"value":"1"},"range":{"value":""},"reload":{"value":""},"rules":[],"size":{"value":"med"},"source":{"value":""},"splashDamage":{"value":null},"stackGroup":{"value":""},"strikingRune":{"value":""},"traits":{"custom":"","rarity":{"value":"uncommon"},"value":["agile","backswing","finesse","nonlethal"]},"usage":{"value":"held-in-one-hand"},"weaponType":{"value":"simple"},"weight":{"value":"L"},"slug":"poi"},"effects":[],"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.PnFfW5u24xZV6mOH"}},"img":"systems/pf2e/icons/equipment/weapons/poi.webp","name":"Poi","permission":{"default":0},"type":"weapon"}
