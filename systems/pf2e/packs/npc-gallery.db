ocation"},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"slug":"mage-hand","source":{"value":"Pathfinder Core Rulebook"},"spellType":{"value":"utility"},"sustained":{"value":false},"target":{"value":"1 unattended object of light Bulk or less"},"time":{"value":"2"},"traditions":{"value":["arcane","occult"]},"traits":{"custom":"","rarity":"common","value":["cantrip"]},"schema":{"version":0.769,"lastMigration":null}},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.pwzdSlJgYqN7bs2w"}},"img":"systems/pf2e/icons/spells/mage-hand.webp","name":"Mage Hand","sort":400000,"type":"spell"},{"_id":"ipz89wB16JTSERLW","data":{"baseItem":null,"containerId":null,"description":{"value":""},"equipped":{"carryType":"worn","handsHeld":1,"invested":null},"equippedBulk":{"value":""},"hardness":0,"hp":{"brokenThreshold":0,"max":0,"value":0},"level":{"value":3},"negateBulk":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":{"gp":6}},"quantity":1,"rules":[],"size":"med","slug":"scholarly-journal","source":{"value":"Pathfinder Core Rulebook"},"stackGroup":null,"traits":{"custom":"","rarity":"common","value":[]},"usage":{"value":"held-in-one-hand"},"weight":{"value":"L"},"schema":{"version":0.769,"lastMigration":null}},"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.hywANJCzT7hMgWna"}},"img":"systems/pf2e/icons/equipment/adventuring-gear/scholarly-journal.webp","name":"Journal","sort":500000,"type":"equipment"},{"_id":"4zzNfH7d9409n3cP","data":{"baseItem":null,"containerId":null,"description":{"value":"<p>Ordinary clothing is functional with basic tailoring, such as peasant garb, monk's robes, or work clothes.</p>"},"equipped":{"carryType":"worn","handsHeld":1,"invested":null},"equippedBulk":{"value":""},"hardness":0,"hp":{"brokenThreshold":0,"max":0,"value":0},"level":{"value":0},"negateBulk":{"value":"0"},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"price":{"value":{"sp":1}},"quantity"