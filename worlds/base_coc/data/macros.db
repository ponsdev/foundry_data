n                }\n            },\n            malfunction: iMalFunc,\n            bullets: iMag,\n            price: {\n                1920: iPrice1920,\n                mdrn: iPriceMdrn\n            },\n            properties: {\n                \"melee\": true,\n                \"rngd\": false,\n                \"shotgun\": false,\n                \"brst\": false,\n                \"auto\": false,\n                \"dbrl\": false,\n                \"thrown\": false,\n                \"impl\": false,\n                \"ahdb\": iahdb,\n                \"addb\": iaddb,\n                \"slnt\": false,\n                \"spcl\": false,\n                \"mont\": false,\n                \"blst\": false,\n                \"stun\": false,\n                \"rare\": false,\n                \"burn\": iburn\n            },\n            eras: {\n                \"1920\": i1920,\n                \"mdrn\": imdrn,\n                \"nvct\": false,\n                \"drka\": false,\n                \"ddts\": false,\n                \"glit\": false,\n                \"pulp\": false\n            }\n        }\n    };\n    await Item.createDocuments([data]);\n}","folder":null,"sort":0,"permission":{"default":0,"aUBcbGh1W4eV7WAv":3},"flags":{}}
