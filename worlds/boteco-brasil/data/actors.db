{"_id":"3gqbpc7VzZGElQT4","name":"Djiago Declamad'Orc","permission":{"default":2,"fNZMR0wRZYko05MN":3,"fLLshg4hZ1PbOpWF":3},"type":"character","data":{"abilities":{"str":{"value":16,"proficient":0},"dex":{"value":12,"proficient":1},"con":{"value":14,"proficient":0},"int":{"value":10,"proficient":0},"wis":{"value":8,"proficient":0},"cha":{"value":15,"proficient":1}},"attributes":{"ac":{"value":10},"hp":{"value":19,"min":0,"max":19,"temp":null,"tempmax":null},"init":{"value":0,"bonus":0},"movement":{"burrow":0,"climb":0,"fly":0,"swim":0,"walk":30,"units":"ft","hover":false},"senses":{"darkvision":60,"blindsight":0,"tremorsense":0,"truesight":0,"units":"ft","special":""},"spellcasting":"cha","death":{"success":0,"failure":0},"encumbrance":{"value":null,"max":null},"exhaustion":0,"inspiration":false},"details":{"biography":{"value":"<p>9 -PASSIVE WISDOM (PERCEPTION)</p>\n<p>11 - PASSIVE WISDOM (INSIGHT)</p>\n<p>10 - PASSIVE INTELLIGENCE (INVESTIGATION)</p>\n<p>&nbsp;</p>\n<p>DARKVISION 60FT</p>\n<p>&nbsp;</p>\n<p>UNARMED STRIKE - +5HIT - 4 BLUDGEONING</p>","public":""},"alignment":"Neutral Good","race":"meio-orc","background":"Artista","xp":{"value":900,"min":0,"max":300},"appearance":"","trait":"","ideal":"","bond":"","flaw":"","attunedItemsMax":"3","attunedItemsCount":0,"maxPreparedSpells":null,"gender":"","age":"","height":"","weight":"","eyes":"","skin":"","hair":"","notes1name":"","notes2name":"","notes3name":"","notes4name":""},"traits":{"size":"med","di":{"value":[],"custom":""},"dr":{"value":[],"custom":""},"dv":{"value":[],"custom":""},"ci":{"value":[],"custom":""},"languages":{"value":["common","orc","undercommon"],"custom":""},"weaponProf":{"value":["sim"],"custom":"Light Crossbow; Mão; Espada Longa"},"armorProf":{"value":["lgt"],"custom":""},"toolProf":{"value":[],"custom":"Gaita Fole; Tambor; Horn; Violino"}},"currency":{"pp":0,"gp":278,"ep":0,"sp":0,"cp":0},"skills":{"acr":{"value":0,"ability":"dex"},"ani":{"value":0,"ability":"wis"},"arc":{"value":0,"abil